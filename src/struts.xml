<struts>
	<package name="fUser" extends = "struts-default" namespace = "/fUser">
		<action name="register" class = "com.flea.action.FUserAction" method = "register">
			<result name = "success" type = "redirect">/Test/test.jsp</result>
			<result name = "error" type = "redirect">/Test/test1.jsp</result>
		</action>
		<action name="emailIsExist" class = "com.flea.action.FUserAction" method = "emailIsExist"></action>
		<action name="userNameIsExist" class = "com.flea.action.FUserAction" method = "userNameIsExist"></action>
	</package>
</struts>
